		initialization();
		loginPage = new LoginPage();
	}
	
	@Test
	public void loginPageTitleTest() {
		loginPage.validateLoginPageTitle();
	}
	
	@AfterTest
	public void tearDown() {
		driver.quit();
	}
	
	
	
}
